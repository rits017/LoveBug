<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgim11"
    tools:context=".MainActivity8">

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="140dp"
        android:layout_height="180dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.065"
        app:srcCompat="@drawable/lbg9" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="270dp"
        android:layout_height="40dp"
        android:text="_gupta.sanskar10"
        android:textColor="#130000"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.992"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.092" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="230dp"
        android:layout_height="40dp"
        android:fontFamily="sans-serif"
        android:text="SANSKAR GUPTA"
        android:textColor="#0E0101"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.779"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.15" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:text="19"
        android:textColor="#0C0101"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.15" />

    <ImageButton
        android:id="@+id/imageButton20"
        android:layout_width="120dp"
        android:layout_height="45dp"
        android:backgroundTint="#E65C8B"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.209"
        app:srcCompat="@android:drawable/ic_menu_manage" />

    <ImageButton
        android:id="@+id/imageButton40"
        android:layout_width="120dp"
        android:layout_height="45dp"
        android:backgroundTint="#E65C8B"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.209"
        app:srcCompat="@android:drawable/ic_dialog_email" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="515dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView24"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:fontFamily="@font/denk_one"
                android:text="To find your match,answer:-"
                android:textColor="#170101"
                android:textSize="30sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView25"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:text="YOU CAN ANSWER THESE QUESTIONS ANY NUMBER OF TIMES."
                android:textColor="#F8EEEE"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView26"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Q1. DO YOU BELEIVE IN LOVE ?"
                android:textColor="#AF003B"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button120"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:text="YES"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button130"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:text="NO"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView27"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Q2. WHAT KIND OF RELATIONSHIP YOU ARE SEEKING ?"
                android:textColor="#AF003B"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button140"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:text="SERIOUS"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button150"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:text="CASUAL"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Q3. WHAT KIND OF PERSON YOU ARE ?"
                android:textColor="#AF003B"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button160"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:text="INTROVERT"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button170"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:text="EXTROVERT"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Q4. WHAT ARE YOU LOOKING FOR ?"
                android:textColor="#AF003B"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button180"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:text="FRIENDSHIP"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button190"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:text="DATING"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView30"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:text="Q5. WHAT'S YOUR RELATIONSHIP STATUS ?"
                android:textColor="#AF003B"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button200"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:text="SINGLE"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button210"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:text="TAKEN"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button220"
                android:layout_width="400dp"
                android:layout_height="45dp"
                android:text="FIND MATCH"
                android:textColor="@color/white"
                android:background="#AF003B"
                android:textSize="20sp"
                android:textStyle="bold" />


        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>